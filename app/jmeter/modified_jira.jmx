        counter = counter + 1;
    }
    custom_fields_to_retain = custom_fields_to_retain.substring(0, custom_fields_to_retain.length() - 1)
}


// request
request_body = request_body + "&amp;" + custom_fields_to_retain + "&amp;" + fields_to_retain
vars.put("p_request_body", request_body.toString())
