        fields_to_retain = fields_to_retain + "fieldsToRetain=" + vars.get("x_fields_to_retain_" + counter) + "&amp;";
        counter = counter + 1;
    }
    fields_to_retain = fields_to_retain.substring(0, fields_to_retain.length() - 1)
}

// dynamic (custom fields)
