              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">def projectId = java.util.concurrent.ThreadLocalRandom.current().nextInt(1, ${NUMBER_OF_PROJECTS} + 1)
def repoId = java.util.concurrent.ThreadLocalRandom.current().nextInt(1, ${NUMBER_OF_REPOS_PER_PROJECT} + 1)
def repoSlug = &quot;${PROJECT_NAME_PREFIX}&quot; + projectId + &quot;-${REPO_NAME_PREFIX}&quot; + repoId
def projectKey = &quot;${PROJECT_NAME_PREFIX}&quot; + projectId

def username = vars.get(&quot;USERNAME&quot;)
def password = vars.get(&quot;USER_PASSWORD&quot;)

vars.put(&quot;REPO_SLUG&quot;, repoSlug)

def gitProtocolSshPercentage = Integer.valueOf(&quot;${GIT_PROTOCOL_SSH_PERCENTAGE}&quot;).intValue()
def randomNum = java.util.concurrent.ThreadLocalRandom.current().nextInt(0, 100)
if (randomNum &lt;= gitProtocolSshPercentage) {
	vars.put(&quot;GIT_PROTOCOL&quot;, &quot;ssh&quot;)
//	vars.put(&quot;REPO_URL&quot;, &quot;ssh://git@${application.ssh_hostname}:${application.ssh_port}/&quot; + projectKey + &quot;/&quot; + repoSlug + &quot;.git&quot;)
	vars.put(&quot;REPO_URL&quot;, &quot;ssh://git@bitbucket-loadbala-6bmjswoyfzm3-5148108.us-east-2.elb.amazonaws.com:7999/&quot; + projectKey + &quot;/&quot; + repoSlug + &quot;.git&quot;)
} else {
	vars.put(&quot;GIT_PROTOCOL&quot;, &quot;http&quot;)
	vars.put(&quot;REPO_URL&quot;, &quot;http://&quot; + username + &quot;:&quot; + password + &quot;@git@bitbucket-loadbala-6bmjswoyfzm3-5148108.us-east-2.elb.amazonaws.com:80/scm/&quot; + projectKey + &quot;/&quot; + repoSlug + &quot;.git&quot;)
}
